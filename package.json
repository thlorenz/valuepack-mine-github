{
  "name": "valuepack-mine-github",
  "version": "0.2.1",
  "description": "Mines github for user and repository data used by valuepack.",
  "main": "index.js",
  "scripts": {
    "test-main": "tap test/*.js",
    "test-0.8": "nave use 0.8 npm run test-main",
    "test-0.10": "nave use 0.10 npm run test-main",
    "test-all": "npm run test-0.8 && npm run test-0.10",
    "test": "if [ -e $TRAVIS ]; then npm run test-all; else npm run test-main; fi",
    "init": "cd ./scripts && ./init.sh"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/valuepack-mine-github.git"
  },
  "homepage": "https://github.com/thlorenz/valuepack-mine-github",
  "dependencies": {
    "request": "~2.21.0",
    "xtend": "~2.0.5",
    "request-all-pages": "~0.2.3",
    "valuepack-core": "~0.3.8"
  },
  "devDependencies": {
    "tap": "~0.4.3",
    "proxyquire": "~0.4.1",
    "level-test": "~1.3.0",
    "level-dump": "~0.4.0",
    "nave": "~0.4.3",
    "level-sublevel": "~4.7.1",
    "levelup": "~0.10.0",
    "leveldown": "~0.6.2"
  },
  "keywords": [
    "mine",
    "github",
    "valuepack",
    "leveldb",
    "level",
    "db",
    "information"
  ],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.6"
  }
}
